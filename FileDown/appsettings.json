{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",

  "AppSetting": {
    "SALT": "salt_key",
    "Secret": "token_secret",
    "TokenExpire_s": 60,
    "RedisServerConnection": "localhost:6379",
    "RedisRetry": 3,
    "ThrottlePerUser": 10
  },

  "DataFactorySetting": {
    "OracleConnectionString": "Data Source=your.ip.address;Persist Security Info=no;User ID=test;Password=test;",
    "OracleConnectionSetting": "Min Pool Size=5;Connection Timeout=60;Incr Pool Size=5; Decr Pool Size=2"
  }
}
